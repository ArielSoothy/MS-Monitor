# Azure Data Explorer Configuration
# Copy this file to .env and fill in your actual values

# Azure AD Authentication
VITE_AZURE_TENANT_ID=your-tenant-id-here
VITE_AZURE_CLIENT_ID=your-client-id-here

# Azure Data Explorer Connection
VITE_AZURE_CLUSTER=msmonitoradx.israelcentral.kusto.windows.net
VITE_AZURE_DATABASE=monitoringdb

# Feature Flags
VITE_ENABLE_AZURE_AUTH=false
VITE_DEMO_MODE=true

# Optional: Azure Function URLs (when deployed)
VITE_INGESTION_FUNCTION_URL=https://your-function-app.azurewebsites.net/api/pipelineDataIngest
VITE_HEALTH_CHECK_URL=https://your-function-app.azurewebsites.net/api/healthCheck

# Development Settings
VITE_LOG_LEVEL=debug
VITE_REFRESH_INTERVAL=30000
